{
    "darkmode": true,
    "themeLocation": "theme.json",
    "pages": [
        "pages/home/index",
        "pages/mine/index",
        "pages/details/index",
        "pages/login/index"
    ],
    "window": {
        "navigationBarBackgroundColor": "@bgColor",
        "navigationBarTextStyle": "@style",
        "navigationBarTitleText": "vue-mini-zhihudaily",
        "backgroundColor": "@bgColor",
        "backgroundColorTop": "@bgColor",
        "backgroundColorBottom": "@bgColor"
    },
    "tabBar": {
        "color": "@color",
        "selectedColor": "@color",
        "backgroundColor": "@bgColor",
        "borderStyle": "@style",
        "list": [
            {
                "text": "日推",
                "pagePath": "pages/home/index",
                "iconPath": "@homeIcon",
                "selectedIconPath": "@homeSelectedIcon"
            },
            {
                "text": "个人",
                "pagePath": "pages/mine/index",
                "iconPath": "@mineIcon",
                "selectedIconPath": "@mineSelectedIcon"
            }
        ]
    },
    "subPackages": [
        {
            "root": "packages/user",
            "pages": ["copyright/index", "profile/index", "favorite/index"]
        }
    ],
    "sitemapLocation": "sitemap.json",
    "lazyCodeLoading": "requiredComponents",
    "resolveAlias": {
        "@/*": "src/*"
    }
}
